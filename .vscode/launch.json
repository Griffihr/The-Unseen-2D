    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run Godot",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "E:/Godot/Godot",
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "args": ["--path","${workspaceRoot}"]
        }
    ]
